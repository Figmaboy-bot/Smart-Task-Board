{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "rewrites": [
    { "source": "/((?!assets|Icons|Social Icons|upcoming deadlines|favicon\\.ico|vite\\.svg|robots\\.txt|manifest\\.json|.*\\..*).*)", "destination": "/index.html" }
  ]
}
